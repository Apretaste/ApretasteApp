<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#FFF"
    style="@style/App"
    tools:context="com.example.apretaste.Settings.Settings_auto_resfrescar">
        <LinearLayout
            android:paddingTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/text_auto_sync"
                android:textColor="#444"
                android:textSize="20sp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:lines="4"
                android:text="@string/description_auto_sync"
                android:textColor="#444"
                android:textSize="15sp"
                android:layout_marginBottom="10dp"
                />
            <com.example.apretaste.AssetDrawableCompatEditText
                android:id="@+id/et_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:drawableRightCompat="@drawable/ic_arrow_drop_down_black_24dp"
                android:focusable="false"
                >
            </com.example.apretaste.AssetDrawableCompatEditText>



        </LinearLayout>




</LinearLayout>
